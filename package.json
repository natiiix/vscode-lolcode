{
    "name": "lolcode",
    "displayName": "LOLCODE",
    "description": "LOLCODE Language Support for Visual Studio Code",
    "version": "1.0.1",
    "author": "natiiix",
    "publisher": "natiiix",
    "contributors": [
        "arwinneil"
    ],
    "license": "MIT",
    "icon": "images/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/natiiix/vscode-lolcode"
    },
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "lolcode",
                "aliases": [
                    "LOLCODE",
                    "Lolcode",
                    "lolcode"
                ],
                "extensions": [
                    ".lol",
                    ".lols"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "lolcode",
                "scopeName": "source.lol",
                "path": "./syntaxes/lol.tmLanguage"
            }
        ]
    }
}
